meta {
  name: list
  type: graphql
  seq: 1
}

post {
  url: {{GRAPHQL_URL}}
  body: graphql
  auth: none
}

body:graphql {
  query {
    reservations {
      id
      startTime
      endTime
      status
      createdAt
      room {
        id
        name
        capacity
        location
        createdAt
      }
      user {
        id
        keycloackId
        email
        createdAt
      }
    }
  }
}
