meta {
  name: list
  type: graphql
  seq: 1
}

post {
  url: {{GRAPHQL_URL}}
  body: graphql
  auth: none
}

body:graphql {
  query {
    rooms {
      id
      name
      capacity
      location
      created_at
      reservations {
        id
        room_id
        user_id
        start_time
        end_time
        status
        created_at
      }
    }
  }
}
